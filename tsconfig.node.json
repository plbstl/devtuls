{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true
  },
  "include": [
    "e2e",
    "plugins",
    "commitlint.config.ts",
    "eslint.config.js",
    "playwright.config.ts",
    "prettier.config.js",
    "vite.config.ts"
  ]
}
